<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nakatsuka.newgit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainAction.MainActivity" />
        <activity
            android:name=".MainAction.SecondActivity"
            android:label="SecondActivity"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="adjustResize"/>
        <activity android:name=".MainAction.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainAction.RuleActivity" />
        <activity android:name=".MainAction.ResistActivity"></activity>
    </application>

</manifest>